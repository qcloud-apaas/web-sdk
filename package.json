{
  "name": "@qcloud-apaas/web-sdk",
  "version": "0.2.4",
  "description": "SDK for apaas component deveopment",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qcloud-apaas/web-sdk"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "build:lib": "tsc -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "build:es": "tsc -d -m esNext --outDir es --sourceMap false --inlineSources false",
    "build:clean": "echo clean",
    "build": "rm -rf lib es && npm run build:lib && npm run build:es && npm run build:clean",
    "scss": "gulp scss && node-sass src --output lib && node-sass src --output es",
    "tsc-check": "tsc --outDir .tsc-check/",
    "eslint": "eslint --ext ts,tsx src",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "dts-bundle": "dts-bundle --configJson dts-bundle.json",
    "pack": "webpack",
    "release": "standard-version",
    "pub": "npm publish",
    "pub:next": "npm publish --tag next"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.14.5",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.1.2",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "final-form": "^4.20.2",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.13.1",
    "prettier": "^2.2.1",
    "react-final-form-hooks": "^2.0.2",
    "standard-version": "^9.0.0",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^26.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.3",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "peerDependencies": {
    "axios": ">=0.20.0",
    "final-form": ">=4.20.0",
    "react": ">=16.8.0",
    "react-final-form-hooks": ">=2.0.0"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7"
  }
}
